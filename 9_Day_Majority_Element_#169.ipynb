    "from collections import Counter\n",
    "array = [3, 3, 3, 3, 3, 3, 2, 4, 4]\n",
    "\n",
    "def lararr(arr):\n",
    "    a = Counter(arr).most_common(1)\n",
    "    if a[0][1] > len(arr)//2:\n",
    "        return (a[0][0])\n",
    "    else:\n",
    "        return None\n",
    "\n",
    "print(lararr(array))"
   ]
